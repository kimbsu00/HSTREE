<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kimbsu.hstree">
    <!-- 인터넷 관련 권한 부여 시작 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 인터넷 관련 권한 부여 끝 -->
    <!-- 전화 다이얼로그 권한 부여 시작 -->
<!--    <uses-permission android:name="android.permission.CALL_PHONE" />-->
    <!-- 전화 다이얼로그 권한 부여 끝 -->

    <application
        android:allowBackup="false"
        android:icon="@mipmap/hstree_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/hstree_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".telephone_info"></activity>
        <activity
            android:name=".loading"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
        <activity android:name=".food_menu" />
        <activity android:name=".update_developer" />
        <activity android:name=".schedule_month_year" />
        <activity android:name=".notice_post" />
        <activity android:name=".notice_list" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>